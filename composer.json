{
    "name": "alphacar/yii2-discovery",
    "description": "yii2 service discovery library",
    "type": "library",
    "autoload": {
        "psr-4": {
            "AlphaCar\\Discovery\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Lion",
            "email": "lion0418@163.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.40",
        "grpc/grpc": "^1.36",
        "google/protobuf": "^3.17",
        "sensiolabs/consul-php-sdk": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
